# Private key for deployment (DO NOT commit actual .env file)
PRIVATE_KEY=your_private_key_here

# Monad RPC URLs
MONAD_TESTNET_RPC_URL=https://testnet-rpc.monad.xyz
MONAD_MAINNET_RPC_URL=https://rpc.monad.xyz

# Monad Block Explorer
MONAD_EXPLORER_API_KEY=your_explorer_api_key
MONAD_TESTNET_EXPLORER=https://testnet-explorer.monad.xyz
MONAD_TESTNET_EXPLORER_API=https://testnet-explorer.monad.xyz/api
MONAD_MAINNET_EXPLORER=https://explorer.monad.xyz
MONAD_MAINNET_EXPLORER_API=https://explorer.monad.xyz/api

# Gas Reporting
REPORT_GAS=false

# Oracle API Keys (for off-chain data)
ORACLE_API_KEY=your_oracle_api_key
FATF_API_KEY=your_fatf_api_key
OFAC_API_KEY=your_ofac_api_key
